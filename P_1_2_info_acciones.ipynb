    "    \"AVGO\",\"ORCL\", \"CRM\",\"AMD\",  \"ACN\", \"CSCO\", \"ADBE\", \"IBM\", \"NOW\", \"QCOM\", \"TXN\", \"INTU\",                                    \n",
    "    \"UBER\", \"AMAT\", \"ANET\", \"PANW\", \"MU\", \"ADP\", \"FI\", \"ADI\", \"INTC\", \"PLTR\", \"LRCX\", \"KLAC\",                             \n",
    "    \"DELL\", \"APH\", \"MSI\", \"SNPS\", \"CRWD\", \"MRVL\", \"CDNS\", \"WDAY\", \"FTNT\", \"ADSK\", \"ROP\", \"NXPI\",                       \n",
