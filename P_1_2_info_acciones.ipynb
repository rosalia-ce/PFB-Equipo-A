   "source": [
    "#datos de cuándo y cuánto ha pagado la empresa en dividendos\n",
    "\n",
    "def get_dividends(ticker):\n",
    "    dividends = yf.Ticker(ticker).dividends\n",
    "    dividends = dividends.reset_index()\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    dividends[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    dividends = pd.DataFrame(dividends)\n",
    "    dividends[\"Extraction_Timestamp\"] = extraction_time\n",
    "    dividends[\"Extraction_Timestamp\"] = pd.to_datetime(dividends[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    dividends.columns = [\"date\", \"dividends\", \"ticker\", \"extraction_timestamp\"]\n",
    "    \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_dividends_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    dividends.to_csv(file_name)\n",
    "\n",
    "    return dividends"
   ]
  },
