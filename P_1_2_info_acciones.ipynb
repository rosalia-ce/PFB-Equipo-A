    "]\n",
    "\n",
    "stock_info = []\n",
    "\n",
    "for ticker in tickers:\n",
    "    stock = yf.Ticker(ticker)\n",
    "    stock_data = {\n",
    "        \"ticker\": ticker,\n",
    "        \"name\": stock.info.get(\"longName\", \"\"),\n",
    "        \"sector\": stock.info.get(\"sector\", \"\"),\n",
    "        \"industry\": stock.info.get(\"industry\", \"\"),\n",
    "        \"market_cap\": stock.info.get(\"marketCap\", \"\"),\n",
    "        \"full_time_employees\": stock.info.get(\"fullTimeEmployees\", \"\"),\n",
    "        \"marketStart\": stock.info.get(\"regularMarketTime\", None)  # Time in Unix\n",
    "        \n",
    "    }\n",
    "    stock_info.append(stock_data)\n",
    "\n",
    "stocks = pd.DataFrame(stock_info)\n",
    "\n",
    "# Convert marketStart from Unix timestamp to human-readable date if needed\n",
    "stocks[\"marketStart\"] = pd.to_datetime(stocks[\"marketStart\"], unit=\"s\")\n",
    "\n",
    "stocks"
