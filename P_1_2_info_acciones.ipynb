    "#datos del comportamiento de la acción a lo largo del tiempo\n",
    "\n",
    "def get_historical_prices(ticker):\n",
    "    historical_prices = yf.download(ticker)\n",
    "    historical_prices = historical_prices.reset_index()\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    historical_prices[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    historical_prices[\"Extraction_Timestamp\"] = extraction_time\n",
    "    historical_prices[\"Extraction_Timestamp\"] = pd.to_datetime(historical_prices[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    historical_prices.columns = [\"date\", \"open\", \"high\", \"low\", \"close\", \"adj_close\", \"volume\", \"ticker\", \"extraction_timestamp\"]\n",
    "    \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_historical_prices_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    historical_prices.to_csv(file_name)\n",
    "\n",
    "    return historical_prices"
   ]
  },
