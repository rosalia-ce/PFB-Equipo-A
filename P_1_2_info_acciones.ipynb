    "#datos de la estructura financiera de la empresa\n",
    "\n",
    "def get_balance_sheet(ticker):\n",
    "    balance_sheet = yf.Ticker(ticker).balance_sheet.T\n",
    "    balance_sheet = balance_sheet.reset_index()\n",
    "    balance_sheet = balance_sheet.rename(columns={'index': 'Date'})\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    balance_sheet[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    balance_sheet[\"Extraction_Timestamp\"] = extraction_time\n",
    "    balance_sheet[\"Extraction_Timestamp\"] = pd.to_datetime(balance_sheet[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #filtrar columnas\n",
    "    balance_sheet_filtered = balance_sheet[[\"Date\", \"ticker\", \"Ordinary Shares Number\", \"Net Debt\", \"Total Assets\", \"Current Liabilities\", \"Stockholders Equity\", \"Extraction_Timestamp\"]]\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    balance_sheet_filtered.columns = [\"date\", \"ticker\", \"ordinary_shares_number\", \"net_debt\", \"total_assets\", \"current_liabilities\", \"stockholders_equity\", \"extraction_timestamp\"]\n",
    "          \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_balance_sheet_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    balance_sheet_filtered.to_csv(file_name)\n",
    "\n",
    "    return balance_sheet_filtered"
   ]
  },
