    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    splits = pd.DataFrame(splits)\n",
    "    splits[\"Extraction_Timestamp\"] = extraction_time\n",
    "    splits[\"Extraction_Timestamp\"] = pd.to_datetime(splits[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    splits.columns = [\"date\", \"splits\", \"ticker\", \"extraction_timestamp\"]\n",
