    "#datos de los cambios en las calificaciones que los analistas financieros hacen sobre una acción\n",
    "\n",
    "def get_upgrades_downgrades(ticker):\n",
    "    upgrades_downgrades = yf.Ticker(ticker).upgrades_downgrades\n",
    "    upgrades_downgrades = upgrades_downgrades.reset_index()\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    upgrades_downgrades[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    upgrades_downgrades[\"Extraction_Timestamp\"] = extraction_time\n",
    "    upgrades_downgrades[\"Extraction_Timestamp\"] = pd.to_datetime(upgrades_downgrades[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    upgrades_downgrades.columns = [\"date\", \"firm\", \"to_grade\", \"from_grade\", \"action\", \"ticker\", \"extraction_timestamp\"]\n",
    "       \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_upgrades_downgrades_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    upgrades_downgrades.to_csv(file_name) \n",
    "\n",
    "    return upgrades_downgrades"
   ]
  },
