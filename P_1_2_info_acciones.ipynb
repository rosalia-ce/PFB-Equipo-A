   "source": [
    "#identificación de las instituciones que tienen participaciones\n",
    "\n",
    "def get_holders(ticker):\n",
    "    holders = yf.Ticker(ticker).institutional_holders\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    holders[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    holders[\"Extraction_Timestamp\"] = extraction_time\n",
    "    holders[\"Extraction_Timestamp\"] = pd.to_datetime(holders[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    holders.columns = [\"date\", \"holder\", \"pct_held\", \"shares\", \"value\", \"ticker\", \"extraction_timestamp\"]\n",
    "        \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_holders_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    holders.to_csv(file_name)\n",
    "\n",
    "    return holders"
   ]
  },
