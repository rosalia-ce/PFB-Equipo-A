    "#muestra cómo se genera y usa el efectivo, sirve para medir su salud financiera\n",
    "\n",
    "def get_cashflow(ticker):\n",
    "    cashflow = yf.Ticker(ticker).cashflow.T\n",
    "    cashflow = cashflow.reset_index()\n",
    "    cashflow = cashflow.rename(columns={'index': 'Date'})\n",
    "    \n",
    "    #añadir valor del ticker para todas las filas\n",
    "    cashflow[\"ticker\"] = ticker\n",
    "    \n",
    "    #añadir timestamp\n",
    "    extraction_time = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    cashflow[\"Extraction_Timestamp\"] = extraction_time\n",
    "    cashflow[\"Extraction_Timestamp\"] = pd.to_datetime(cashflow[\"Extraction_Timestamp\"])\n",
    "    \n",
    "    #filtrar columnas\n",
    "    cashflow_filtered = cashflow[[\"Date\", \"ticker\", \"Operating Cash Flow\", \"Capital Expenditure\", \"Free Cash Flow\", \"Extraction_Timestamp\"]]\n",
    "    \n",
    "    #renombrar las columnas\n",
    "    cashflow_filtered.columns = [\"date\", \"ticker\", \"operating_cash_flow\", \"capital_expenditure\", \"free_cash_flow\", \"extraction_timestamp\"]\n",
    "    \n",
    "    #guardar .csv con el nombre del ticker y la fecha de extracción\n",
    "    file_name = f\"{ticker}_cashflow_{datetime.now().strftime('%Y%m%d')}.csv\"\n",
    "    cashflow_filtered.to_csv(file_name)\n",
    "\n",
    "    return cashflow_filtered"
   ]
  },
